parameters:
    level: 7
    paths:
        - module
    ignoreErrors:
        - '#Method [A-Za-z]+\\Module::onBootstrap\(\) has parameter \$e with generic interface Laminas\\EventManager\\EventInterface but does not specify its types: TTarget, TParams#'
        - identifier: missingType.iterableValue
    excludePaths:
        - module/Application/src/Controller/IndexController.php
    bootstrapFiles:
        - create_documentation.php
